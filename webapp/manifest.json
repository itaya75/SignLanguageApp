{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "sign",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.34.5"
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "sign.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.34.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.34.0"
				},
				"sap.m": {
					"minVersion": "1.34.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sign.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sign.view",
				"controlId": "idAppControl",
				"controlAggregation": "detailPages",				
				"bypassed": {
					"target": ["master"]
				}
			},

			"routes": [{
				"pattern": "",
				"name": "master",
				"target": ["category", "master"],
				"greedy": false
			}, {
				"pattern": "categories/{categoryId}",
				"name": "category",
				"target": ["master", "category"]
			},
				{
				"pattern": "category/{categoryId}/word/{wordId}",
				"name": "word",
				"target": ["word"]
				}
			],

			"targets": {
				"master": {
					"viewType": "XML",
					"viewLevel": 1,
					"viewName": "Master",
					"controlAggregation": "masterPages",
					"viewId": "master",
					"viewPath": "sign.view"
				},
				"category": {
					"viewType": "XML",
					"viewLevel": 2,
					"viewName": "Category",
					"controlAggregation": "masterPages",
					"viewId": "category",
					"viewPath": "sign.view"
				},
				"word": {
					"viewType": "XML",
					"viewLevel": 3,
					"viewName": "Word",
					"controlAggregation": "detailPages",
					"viewId": "word",
					"viewPath": "sign.view"
				}
			}
		}
	}
}