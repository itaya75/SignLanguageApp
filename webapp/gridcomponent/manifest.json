{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "sign",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.34.5"
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "sign.gridcomponent.view.FullScreenApp",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.34.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.34.0"
				},
				"sap.m": {
					"minVersion": "1.34.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sign.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sign.gridcomponent.view",
				"controlId": "fullScreenAppId",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["master"]
				}
			},
			"routes": [{
				"pattern": "",
				"name": "welcome",
				"target": ["welcome"],
				"greedy": false
			}, {
				"pattern": "settings",
				"name": "settings",
				"target": ["settings"],
				"greedy": false
			}, {
				"pattern": "mastergrid",
				"name": "mastergrid",
				"target": ["mastergrid"],
				"greedy": false
			}, {
				"pattern": "grid/categories/{categoryId}",
				"name": "categorygrid",
				"target": ["categorygrid"]
			}, {
				"pattern": "grid/category/{categoryId}/word/{wordId}",
				"name": "wordgrid",
				"target": ["wordgrid"]
			}],
			"targets": {
				"welcome": {
					"viewType": "XML",
					"viewLevel": 1,
					"viewName": "Welcome",
					"controlAggregation": "pages",
					"controlId": "fullScreenAppId",
					"viewId": "welcome",
					"viewPath": "sign.gridcomponent.view"
				},
				"settings": {
					"viewType": "XML",
					"viewLevel": 1,
					"viewName": "Settings",
					"controlAggregation": "pages",
					"controlId": "fullScreenAppId",
					"viewId": "settings",
					"viewPath": "sign.gridcomponent.view"
				},
				"mastergrid": {
					"viewType": "XML",
					"viewLevel": 1,
					"viewName": "MasterGrid",
					"controlAggregation": "pages",
					"controlId": "fullScreenAppId",
					"viewId": "masterGrid",
					"viewPath": "sign.gridcomponent.view"
				},
				"categorygrid": {
					"viewType": "XML",
					"viewLevel": 1,
					"viewName": "CategoryGrid",
					"controlAggregation": "pages",
					"controlId": "fullScreenAppId",
					"viewId": "categorygrid",
					"viewPath": "sign.gridcomponent.view"
				},
				"wordgrid": {
					"viewType": "XML",
					"viewLevel": 3,
					"viewName": "WordGrid",
					"controlAggregation": "pages",
					"controlId": "fullScreenAppId",
					"viewId": "wordgrid",
					"viewPath": "sign.gridcomponent.view"
				}
			}
		}
	}
}